<fx:root prefHeight="510.0" prefWidth="891.0" type="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="tbvRoles" onMouseClicked="#selectRole" prefHeight="256.0" prefWidth="1047.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <columns>
            <TableColumn editable="false" minWidth="146.0" prefWidth="219.0" text="Role" />
            <TableColumn editable="false" minWidth="152.0" prefWidth="152.0" resizable="false" sortable="false" style="-fx-alignment: CENTER;" text="Using Password" />
         </columns>
      </TableView>
      <HBox alignment="CENTER_RIGHT" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button mnemonicParsing="false" onAction="#newRole" prefHeight="27.0" prefWidth="120.0" text="New" textFill="#03a400" />
            <Button mnemonicParsing="false" prefHeight="27.0" prefWidth="120.0" text="Delete" textFill="#bf0000" />
            <Button mnemonicParsing="false" onAction="#update" prefHeight="27.0" prefWidth="120.0" text="Update" />
            <Button mnemonicParsing="false" onAction="#reload" prefHeight="27.0" prefWidth="120.0" text="Refresh" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
