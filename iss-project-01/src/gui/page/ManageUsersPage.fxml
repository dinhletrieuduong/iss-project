<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<fx:root prefHeight="423.0" prefWidth="795.0" type="AnchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="tbvUsers" onMouseClicked="#selectUser" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <columns>
            <TableColumn editable="false" minWidth="182.0" prefWidth="182.0" sortable="false" text="Username" />
            <TableColumn editable="false" minWidth="155.0" prefWidth="155.0" text="Created" />
            <TableColumn editable="false" minWidth="158.0" prefWidth="158.0" resizable="false" style="-fx-alignment: CENTER;" text="Using Password" />
         </columns>
      </TableView>
      <HBox alignment="CENTER" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button mnemonicParsing="false" onAction="#newUser" prefHeight="27.0" prefWidth="120.0" text="New" textFill="#21b929" />
            <Button mnemonicParsing="false" onAction="#delUser" prefHeight="27.0" prefWidth="120.0" text="Delete" textFill="#941d1d" />
            <Button mnemonicParsing="false" onAction="#setPassword" prefHeight="27.0" prefWidth="120.0" text="Update" />
            <Button mnemonicParsing="false" onAction="#refresh" prefHeight="27.0" prefWidth="120.0" text="Refresh" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
